/**app.wxss**/
@import "style/iconfont.wxss";
.false-tab-icon{
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    bottom: 0;
    z-index: 99999;
    width: 33.33%;
    height: 150rpx;
}
page{
    height: 100%;
}
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
} 

button {
    background: initial;
}

button:focus{
    outline: 0;
}

button::after{
    border: none;
}
.artic{
    width: 100%;
    flex: 1;
    background-color: #f4f4f4;
}
.my-head{
    width: 100%;
}
.page-title{
    width: 100%;
    margin-top: 18rpx;
    text-align: center;
    font-weight: 500;
    font-size: 32rpx;
}